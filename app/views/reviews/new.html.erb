<h1>Add a new review for <%= @restaurant.name %></h1>
<%= simple_form_for [@restaurant, @review] do |f| %>
  <%= f.input :content %>
  <%= f.input :rating, as: :radio_buttons, co12llection: [1, 2, 3, 4, 5] %>
  <%= f.submit %>
<% end %>
<%= link_to "Back to restaurant", restaurant_path(@restaurant), class: "btn btn-primary"  %>
